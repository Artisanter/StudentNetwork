@model IEnumerable<StudentNetwork.Models.Student>

@{
    ViewData["Title"] = @Localizer["Search"];
}

<h3>Search</h3>

<form asp-controller="Students" asp-action="@Localizer["Search"]">
    <p>
        <input type="text" name="searchString">
        <input type="submit" value="@Localizer["Search"]" />
    </p>
</form>

@foreach (var item in Model)
{
    @await Html.PartialAsync("_Details", item)
}