@model Student
<table class="table">
    <tbody>
        <tr>
            <td style="width: 120px;">
                <a asp-action="Index" asp-controller="Account" asp-route-id="@Model.Id">
                    <picture image="Model.Image" round="true" />
                </a>
            </td>
            <td>
                <a asp-action="Index" asp-controller="Account" asp-route-id="@Model.Id" style="margin: 10px">
                    @Html.DisplayFor(modelItem => Model.Name)
                </a>
            </td>
        </tr>
    </tbody>
</table>