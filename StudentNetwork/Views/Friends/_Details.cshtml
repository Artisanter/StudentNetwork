@model Student
<tr>
    <td style="width: 120px;">
        <a asp-action="Index" asp-controller="Account" asp-route-id="@Model.Id">
            @Html.Raw("<img style='width: 100px;height: 100px;border-radius: 50%;' src=\"data:image/jpeg;base64,"
                                        + Convert.ToBase64String(Model.Image.Bytes) + "\" />")
        </a>
    </td>
    <td>
        <a asp-action="Index" asp-controller="Account" asp-route-id="@Model.Id" style="margin: 10px">
            @Html.DisplayFor(modelItem => Model.Name)
        </a>
    </td>
</tr>